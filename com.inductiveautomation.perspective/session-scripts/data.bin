{"onBarcodeDataReceived":"\t","onBluetoothReceived":"\t","onAccelerometerDataReceived":"\t","onNdefDataReceived":"\t","messageHandlers":[{"name":"userCreated","enabled":true,"script":"# \t\t    message = payload['message']\n# \t\t    sessionId = session.props.id\n# \t\t\n# \t\t    tagPath = \"[default]docType\"\n# \t\t    currentValue = system.tag.read(tagPath).value[\"userArray\"]\n#\n#    \t\t# Append the message to the end of the current value array\n#    \t\tcurrentValue.append(message)\n#\n#    \t\t# Write the updated array back to the tag\n#    \t\tsystem.tag.write(tagPath, {\"userArray\": currentValue})\n#\n# \t\t    logger = system.util.getLogger(\"myLogger\")\n#\t\t\t#logger.info(str(currentValue))\n# \t\t    logger.info(str(message))\n    \tmessage = payload['message']\n        sessionId = session.props.id\n    \n        tagPath = \"[default]docType\"\n        currentValue = list(system.tag.read(tagPath).value[\"userArray\"])  # Convert array to list\n    \n        # Append the message to the end of the current value list\n        currentValue.append(message)\n    \n        # Write the updated list back to the tag\n        system.tag.write(tagPath, {\"userArray\": currentValue})\n    \n        logger = system.util.getLogger(\"myLogger\")\n        logger.info(str(message))"}]}